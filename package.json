    "url": "https://yequanrui.github.io"
  },
  "homepage": "https://github.com/yequanrui/yequanrui.github.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/yequanrui/yequanrui.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/yequanrui/yequanrui.github.io/issues"
  }
